        text-align: center;
      }

      .card-heading {
        font-family: "FranziskaWebPro";
        font-style: italic;
        margin: 0;
        line-height: 1.15;
        font-weight: bold;
      }

      .card-description {
        font-family: "TabletGothic";
        position: relative;
        background-color: transparent;
        text-align: center;
        text-transform: none;
        letter-spacing: 0.8px;
      }

