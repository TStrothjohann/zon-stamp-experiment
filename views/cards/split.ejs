<amp-story-page id="<%= id %>">
  <amp-story-grid-layer template="thirds">
    <div grid-area="upper-third" class="image-overlay center-text">
      <p class="quote__name"><%= card.divider %></p>
    </div>
    <div grid-area="middle-third" class="image-overlay center-text">
      <p class="quote__name"><%= card.headingAlpha %></p>
    </div>
    <div grid-area="lower-third" class="image-overlay center-text">
      <p class="quote__name"><%= card.headingBeta %></p>
    </div>
  </amp-story-grid-layer>
</amp-story-page>